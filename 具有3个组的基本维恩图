# Import the library
import matplotlib.pyplot as plt
from matplotlib_venn import venn3

# Make the diagram
ax1 = venn3(subsets=(10, 8, 22, 6, 9, 4, 2)); #A, B, AB, C, AC, AB, ABC
plt.show()

# 设置三组ABCD、DEF、ADG
ax2 = venn3([set(['A', 'B', 'C', 'D']), set(['D', 'E', 'F']), set(['A', 'D', 'G','F'])]);
plt.show()

